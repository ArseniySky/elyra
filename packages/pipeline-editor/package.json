{
  "name": "@elyra/pipeline-editor-extension",
  "version": "2.0.0-dev",
  "description": "JupyterLab extension - Visual editor to build Notebook pipelines",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/elyra-ai/elyra",
  "bugs": {
    "url": "https://github.com/elyra-ai/elyra/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elyra-ai/elyra"
  },
  "license": "Apache-2.0",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf myextension/static",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jupyter labextension develop --overwrite .",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@elyra/application": "^2.0.0-dev",
    "@elyra/canvas": "9.0.3",
    "@elyra/metadata-common": "^2.0.0-dev",
    "@elyra/ui-components": "^2.0.0-dev",
    "@jupyterlab/application": "^3.0.0-rc.5",
    "@jupyterlab/apputils": "^3.0.0-rc.5",
    "@jupyterlab/docregistry": "^3.0.0-rc.5",
    "@jupyterlab/filebrowser": "^3.0.0-rc.5",
    "@jupyterlab/filebrowser-extension": "^3.0.0-rc.5",
    "@jupyterlab/launcher": "^3.0.0-rc.5",
    "@jupyterlab/notebook": "^3.0.0-rc.5",
    "@jupyterlab/outputarea": "^3.0.0-rc.5",
    "@jupyterlab/ui-components": "^3.0.0-rc.5",
    "@lumino/algorithm": "^1.3.3",
    "@lumino/coreutils": "^1.5.3",
    "@lumino/disposable": "^1.4.3",
    "@lumino/dragdrop": "^1.6.4",
    "@lumino/widgets": "^1.14.0",
    "@material-ui/core": "^4.1.2",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/lab": "^4.0.0-alpha.18",
    "@types/node": "^12.0.10",
    "@types/uuid": "^3.4.7",
    "autoprefixer": "^9.6.0",
    "carbon-components": "~10.8.1",
    "json-loader": "^0.5.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^3.0.0",
    "react-redux": "^7.2.0",
    "react-scripts": "3.0.1",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0-rc.2",
    "@jupyterlab/testutils": "^1.0.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^23.3.11",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/react-intl": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "install": "^0.13.0",
    "jest": "^24.7.1",
    "jest-raw-loader": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.0",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.2.1",
    "typescript": "~4.0.3"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "elyra_pipeline_editor_extension/static"
  },
  "publishConfig": {
    "access": "public"
  }
}
